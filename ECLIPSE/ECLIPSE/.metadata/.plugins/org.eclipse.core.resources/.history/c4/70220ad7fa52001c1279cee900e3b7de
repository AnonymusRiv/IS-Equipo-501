#include "Usuario_Normal.h"
#include <string>

Usuario_normal::UsuarioNormal(int id, string name, string password, string mail, int nucleus, int time){
	UN_ID_=id;
	UN_Name_=name;
	UN_Password_=password;
	UN_Mail_=mail;
	UN_Nucleus_=nucleus;
	N_Time=time;
}
int getId();
string getPassword();
bool setPassword();
string getName();
bool setName();
string getMail();
bool setMail();
int getNucleus();
int getTime();
