#include <string>
using namespace std;

class Usuario_administrador{
	private:
		int UA_ID_;
		string UA_Password_;
		string UA_Name_;
		string UA_Mail_;

		string getPassword();
	public:
		int getId();
		string getName();
		string getMail();
		bool setPassword();
		bool setMail();
		bool setPassword();
		bool setName();
};
class Usuario_administrador_de_usuarios : public Usuario_administrador{
	private:
		int UA_Type_;
	public:
		Usuario_administrador_de_usuarios(int id, string password, string name, string mail, int type=1);
};
