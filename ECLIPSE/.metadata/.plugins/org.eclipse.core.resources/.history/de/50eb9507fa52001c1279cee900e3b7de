#include "string"
using namespace std;

class Usuario_normal{
	private:
		int UN_ID_;
		string UN_Name_;
		string UN_Password_;
		string UN_Mail_;
		int UN_Nucleus_;
		int N_Time;
	public:
		UsuarioNormal(int id, string name, string password, string mail, int nucleus=0, int time=0);
		int getId();
		string getPassword();
		bool setPassword();
		string getName();
		bool setName();
		string getMail();
		bool setMail();
		int getNucleus();
		int getTime();
};
