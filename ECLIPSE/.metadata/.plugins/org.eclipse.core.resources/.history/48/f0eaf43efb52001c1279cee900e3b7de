#include "Usuario_Normal.h"
#include <string>

Usuario_normal::UsuarioNormal(int id, string name, string password, string mail, int nucleus, int time){
	UN_ID_=id;
	UN_Name_=name;
	UN_Password_=password;
	UN_Mail_=mail;
	UN_Nucleus_=nucleus;
	N_Time=time;
}
inline int getId(){return UN_ID_; };
string getPassword(){return UN_Password_;};
bool setPassword();
inline string getName(){return UN_Name_;};
bool setName();
inline string getMail(){return UN_Mail_;};
bool setMail();
inline int getNucleus(){return UN_Nucleus_;};
inline int getTime(){return UN_Time_;};
